  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
